    "dev": "ts-node-dev --respawn --transpile-only src/main/server.ts",
    "up": "docker-compose up -d",
    "build": "tsc",
    "start": "nodemon --exec node dist/main/server.js",
    "typeorm:run": "npx ts-node-dev --transpile-only ./node_modules/typeorm/cli.js migration:run",
    "typeorm:rever": "npx ts-node-dev --transpile-only ./node_modules/typeorm/cli.js migration:revert",
    "typeorm:gen": "npx ts-node-dev --transpile-only ./node_modules/typeorm/cli.js migration:generate -- -n createClass1",
    "typeorm:cre": "npx ts-node-dev --transpile-only ./node_modules/typeorm/cli.js migration:create -n createClass1"
  },
