{
  "name": "teste",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/main/server.ts",
    "down": "docker-compose down && rm -rf dist && rm tsconfig.tsbuildinfo",
    "build": "tsc",
    "start:dev": "docker-compose up -d && ts-node-dev --respawn --transpile-only src/main/server.ts",
    "start": "node dist/main/server.js",
    "test": "jest --watchAll",
    "test:staged": "jest --passWithNoTests",
    "test:ci": "jest --coverage",
    "test:coveralls": "jest --coverage && coveralls < coverage/lcov.info",
    "typeorm:run": "npx ts-node-dev --transpile-only ./node_modules/typeorm/cli.js migration:run",
    "typeorm:rever": "npx ts-node-dev --transpile-only ./node_modules/typeorm/cli.js migration:revert",
    "typeorm:gen": "npx ts-node-dev --transpile-only ./node_modules/typeorm/cli.js migration:generate -- -n createClass1",
    "typeorm:cre": "npx ts-node-dev --transpile-only ./node_modules/typeorm/cli.js migration:create -n createClass1"

    
  },
  "keywords": [
    "clean Architecture",
    "clean-code",
    "test"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.2",
    "@types/node": "^16.7.5",
    "@types/node-fetch": "^2.5.12",
    "@types/pg": "^8.6.0",
    "@types/uuid": "^8.3.0",
    "@types/validator": "^13.1.4",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.29.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "jest": "^27.0.4",
    "ts-jest": "^27.0.3",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.3.4"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "discord.js": "^13.1.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "jsonwebtoken": "^8.5.1",
    "passport": "^0.4.1",
    "passport-discord": "^0.1.4",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.37",
    "uuid": "^3.4.0",
    "validator": "^13.6.0"
  }
}
